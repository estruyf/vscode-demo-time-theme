{
  "$schema": "vscode://schemas/color-theme",
  "name": "Demo Time Dark",
  "type": "dark",
  "colors": {
    "editor.background": "#15181f",
    "editor.foreground": "#eaeef6",
    "editorCursor.foreground": "#ffd43b",
    "editor.selectionBackground": "#ffd43b44",
    "editor.selectionHighlightBackground": "#ffd43b22",
    "editor.lineHighlightBackground": "#202736",
    "editorLineNumber.foreground": "#6b7280",
    "editorLineNumber.activeForeground": "#ffd43b",
    "editorIndentGuide.background1": "#2d3142",
    "editorIndentGuide.activeBackground1": "#505869",
    "editorRuler.foreground": "#2d3142",
    "editorWhitespace.foreground": "#2d3142",
    "editorBracketMatch.background": "#ffd43b22",
    "editorBracketMatch.border": "#ffd43b",
    "editorGroupHeader.tabsBackground": "#202736",
    "editorGroupHeader.tabsBorder": "#2d3142",

    "activityBar.background": "#202736",
    "activityBar.foreground": "#eaeef6",
    "activityBar.border": "#2d3142",
    "activityBarBadge.background": "#ffd43b",
    "activityBarBadge.foreground": "#15181f",
    "activityBar.activeBorder": "#ffd43b",
    "activityBar.activeFocusBorder": "#ffd43b",
    "activityBarTop.activeBorder": "#ffd43b",

    "sideBar.background": "#202736",
    "sideBar.foreground": "#eaeef6",
    "sideBar.border": "#374151",
    "sideBarTitle.foreground": "#eaeef6",
    "sideBarSectionHeader.background": "#2d3142",
    "sideBarSectionHeader.foreground": "#eaeef6",

    "list.activeSelectionBackground": "#ffd43b44",
    "list.activeSelectionForeground": "#ffffff",
    "list.inactiveSelectionBackground": "#2d3142",
    "list.hoverBackground": "#2d3142",
    "list.focusBackground": "#ffd43b22",
    "list.focusOutline": "#ffd43b",

    "statusBar.background": "#202736",
    "statusBar.foreground": "#eaeef6",
    "statusBar.border": "#374151",
    "statusBarItem.hoverBackground": "#2d3142",

    "titleBar.activeBackground": "#202736",
    "titleBar.activeForeground": "#eaeef6",
    "titleBar.inactiveBackground": "#15181f",
    "titleBar.inactiveForeground": "#9ba4b7",
    "titleBar.border": "#374151",

    "button.background": "#ffd43b",
    "button.foreground": "#15181f",
    "button.hoverBackground": "#e6be36",

    "input.background": "#2d3142",
    "input.foreground": "#eaeef6",
    "input.border": "#6b7280",
    "input.placeholderForeground": "#9ba4b7",
    "inputOption.activeBorder": "#ffd43b",

    "dropdown.background": "#2d3142",
    "dropdown.foreground": "#eaeef6",
    "dropdown.border": "#505869",

    "tab.activeBackground": "#15181f",
    "tab.activeForeground": "#ffffff",
    "tab.activeBorder": "#ffd43b",
    "tab.inactiveBackground": "#202736",
    "tab.inactiveForeground": "#9ba4b7",
    "tab.border": "#374151",

    "breadcrumb.background": "#15181f",
    "breadcrumb.foreground": "#9ba4b7",
    "breadcrumb.focusForeground": "#eaeef6",
    "breadcrumb.activeSelectionForeground": "#ffd43b",

    "panel.background": "#202736",
    "panel.border": "#374151",
    "panelTitle.activeForeground": "#eaeef6",
    "panelTitle.inactiveForeground": "#9ba4b7",
    "panelTitle.activeBorder": "#ffd43b",

    "terminal.background": "#15181f",
    "terminal.foreground": "#eaeef6",
    "terminal.ansiBlack": "#15181f",
    "terminal.ansiRed": "#ff6b6b",
    "terminal.ansiGreen": "#51cf66",
    "terminal.ansiYellow": "#ffd43b",
    "terminal.ansiBlue": "#74c0fc",
    "terminal.ansiMagenta": "#d0bfff",
    "terminal.ansiCyan": "#66d9ef",
    "terminal.ansiWhite": "#eaeef6",

    "gitDecoration.modifiedResourceForeground": "#ffd43b",
    "gitDecoration.deletedResourceForeground": "#ff6b6b",
    "gitDecoration.untrackedResourceForeground": "#51cf66",
    "gitDecoration.ignoredResourceForeground": "#9ba4b7",
    "gitDecoration.conflictingResourceForeground": "#d0bfff",

    "diffEditor.insertedTextBackground": "#51cf6622",
    "diffEditor.removedTextBackground": "#ff6b6b22",

    "debugToolBar.background": "#202736",
    "debugToolBar.border": "#374151",

    "notificationCenter.border": "#6b7280",
    "notificationToast.border": "#6b7280",

    "menu.background": "#2d3142",
    "menu.foreground": "#eaeef6",
    "menu.border": "#6b7280",
    "menu.selectionBackground": "#ffd43b44",
    "menu.selectionForeground": "#ffffff",

    "menubar.selectionBackground": "#ffd43b44",
    "menubar.selectionForeground": "#ffffff",

    "quickInput.background": "#202736",
    "quickInput.foreground": "#eaeef6",
    "quickInputTitle.background": "#202736",
    "quickInputList.focusBackground": "#ffd43b44",
    "quickInputList.focusForeground": "#eaeef6",
    "pickerGroup.border": "#374151",
    "pickerGroup.foreground": "#a8b2c4"
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": "comment",
      "settings": {
        "foreground": "#a8b2c4",
        "fontStyle": "italic"
      }
    },
    {
      "name": "String",
      "scope": "string",
      "settings": {
        "foreground": "#ffd43b"
      }
    },
    {
      "name": "Number",
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#c4c8d3"
      }
    },
    {
      "name": "Boolean",
      "scope": "constant.language.boolean",
      "settings": {
        "foreground": "#ff6b6b"
      }
    },
    {
      "name": "Keyword",
      "scope": "keyword",
      "settings": {
        "foreground": "#ff8fb3",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Storage type",
      "scope": "storage.type",
      "settings": {
        "foreground": "#74c0fc",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Function name",
      "scope": ["entity.name.function", "support.function"],
      "settings": {
        "foreground": "#ffd43b"
      }
    },
    {
      "name": "Class name",
      "scope": "entity.name.class",
      "settings": {
        "foreground": "#66d9ef"
      }
    },
    {
      "name": "Type name",
      "scope": "entity.name.type",
      "settings": {
        "foreground": "#66d9ef"
      }
    },
    {
      "name": "Variable",
      "scope": "variable",
      "settings": {
        "foreground": "#eaeef6"
      }
    },
    {
      "name": "Property",
      "scope": "variable.other.property",
      "settings": {
        "foreground": "#eaeef6"
      }
    },
    {
      "name": "Parameter",
      "scope": "variable.parameter",
      "settings": {
        "foreground": "#d0bfff"
      }
    },
    {
      "name": "Tag",
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#74c0fc"
      }
    },
    {
      "name": "Attribute",
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#51cf66"
      }
    },
    {
      "name": "Operator",
      "scope": "keyword.operator",
      "settings": {
        "foreground": "#c4c8d3"
      }
    },
    {
      "name": "JSON Key - Level 0",
      "scope": [
        "source.json meta.structure.dictionary.json support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#ffd43b"
      }
    },
    {
      "name": "JSON String Values",
      "scope": ["source.json string.quoted.double.json"],
      "settings": {
        "foreground": "#8bb3ff"
      }
    },
    {
      "name": "JSON Numbers",
      "scope": ["source.json constant.numeric.json"],
      "settings": {
        "foreground": "#d0bfff"
      }
    },
    {
      "name": "JSON Boolean and Null",
      "scope": ["source.json constant.language.json"],
      "settings": {
        "foreground": "#ff6b6b"
      }
    },
    {
      "name": "JSON Punctuation",
      "scope": [
        "source.json punctuation.separator.dictionary.key-value.json",
        "source.json punctuation.separator.dictionary.pair.json",
        "source.json punctuation.separator.array.json"
      ],
      "settings": {
        "foreground": "#c4c8d3"
      }
    },
    {
      "name": "Markdown - Heading 1",
      "scope": ["heading.1.markdown", "entity.name.section.markdown"],
      "settings": {
        "foreground": "#ffd43b",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown - Heading 2",
      "scope": ["heading.2.markdown"],
      "settings": {
        "foreground": "#e6be36",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown - Heading 3",
      "scope": ["heading.3.markdown"],
      "settings": {
        "foreground": "#ccac30",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown - Heading 4-6",
      "scope": [
        "heading.4.markdown",
        "heading.5.markdown",
        "heading.6.markdown"
      ],
      "settings": {
        "foreground": "#b39629",
        "fontStyle": "bold"
      }
    }
  ]
}
