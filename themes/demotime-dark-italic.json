  "tokenColors": [
    {
      "scope": ["comment", "punctuation.definition.comment", "string.comment"],
      "settings": {
        "foreground": "#a8b2c4",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["constant.other.placeholder", "constant.character"],
      "settings": {
        "foreground": "#ED217C"
      }
    },
    {
      "scope": [
        "constant",
        "entity.name.constant",
        "variable.other.constant",
        "variable.other.enummember",
        "variable.language",
        "entity"
      ],
      "settings": {
        "foreground": "#74c0fc",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "entity.name",
        "meta.export.default",
        "meta.definition.variable"
      ],
      "settings": {
        "foreground": "#ffd43b"
      }
    },
    {
      "scope": [
        "variable.parameter.function",
        "meta.jsx.children",
        "meta.block",
        "meta.tag.attributes",
        "entity.name.constant",
        "meta.object.member",
        "meta.embedded.expression"
      ],
      "settings": {
        "foreground": "#D9DBE1"
      }
    },
    {
      "scope": "entity.name.function",
      "settings": {
        "foreground": "#d0bfff",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["entity.name.tag", "support.class.component"],
      "settings": {
        "foreground": "#7ee787"
      }
    },
    {
      "scope": "keyword",
      "settings": {
